[
    {"regex":"^exit$", "script":"exit_fn()"},
    {"regex":"^#[0-9a-fA-F]{6}$", "script":"color = as_rgb(cmd)"},
    {"regex":"^print_init$", "script":"print_init()"},
    {"regex":"^([hjkl]([1-9][0-9]*)*)+$", "script":"cursor = cursor+as_pos(cmd+\"#\")"},
    {"regex":"^x(0|[1-9][0-9]*)$", "script":"cursor = move_to(cursor,cmd)"},
    {"regex":"^y(0|[1-9][0-9]*)$", "script":"cursor = move_to(cursor,cmd)"},
    {"regex":"^x(0|[1-9][0-9]*)y(0|[1-9][0-9]*)$", "script":"cursor = move_to(cursor,cmd)"},
    {"regex":"^y(0|[1-9][0-9]*)x(0|[1-9][0-9]*)$", "script":"cursor = move_to(cursor,cmd)"}
]
[
    {
        "regex": "^exit$",
        "script": "exit_fn()"
    },
    {
        "regex": "^#[0-9a-fA-F]{6}$",
        "script": "color = as_rgb(cmd)"
    },
    {
        "regex": "^print_init$",
        "script": "a::print_init()"
    },
    {
        "regex": "^print_up$",
        "script": "b::print_up();b::print_up();"
    },
    {
        "regex": "^([hjkl]([1-9][0-9]*)*)+$",
        "script": "cursor = cursor + as_pos(cmd+\"#\")"
    },
    {
        "regex": "^x(0|[1-9][0-9]*)$",
        "script": "cursor = move_to(cursor, cmd)"
    },
    {
        "regex": "^y(0|[1-9][0-9]*)$",
        "script": "cursor = move_to(cursor, cmd)"
    },
    {
        "regex": "^x(0|[1-9][0-9]*)y(0|[1-9][0-9]*)$",
        "script": "cursor = move_to(cursor, cmd)"
    },
    {
        "regex": "^y(0|[1-9][0-9]*)x(0|[1-9][0-9]*)$",
        "script": "cursor = move_to(cursor, cmd)"
    },
    {
        "regex": "^d([hjkl]([1-9][0-9]*)*)*$",
        "script": "let cursor = draw(canvas, cursor, color, cmd+\"#\");"
    },
    {
        "regex": "^save$",
        "script": "save(canvas, \"test.png\");"
    }
]

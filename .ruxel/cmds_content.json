[
    {
        "regex": "^exit$",
        "script": "exit_fn()"
    },
    {
        "regex": "^#[0-9a-fA-F]{6}$",
        "script": "color = as_rgb(cmd)"
    },
    {
        "regex": "^print_init$",
        "script": "a::print_init()"
    },
    {
        "regex": "^print_up$",
        "script": "b::print_up();b::print_up();"
    },
    {
        "regex": "^(([1-9][0-9]*)*[hjkl])+$",
        "script": "cursor = cursor + as_pos(cmd)"
    },
    {
        "regex": "^x(0|[1-9][0-9]*)$",
        "script": "cursor = move_to(cursor, cmd)"
    },
    {
        "regex": "^y(0|[1-9][0-9]*)$",
        "script": "cursor = move_to(cursor, cmd)"
    },
    {
        "regex": "^x(0|[1-9][0-9]*)y(0|[1-9][0-9]*)$",
        "script": "cursor = move_to(cursor, cmd)"
    },
    {
        "regex": "^y(0|[1-9][0-9]*)x(0|[1-9][0-9]*)$",
        "script": "cursor = move_to(cursor, cmd)"
    },
    {
        "regex": "^d(([1-9][0-9]*)*[hjkl])*$",
        "script": "let cursor = draw(canvas, cursor, color, cmd);"
    },
    {
        "regex": "^save$",
        "script": "save(canvas, \"test.png\");"
    },
    {
        "regex": "^rd$",
        "script": "let render_pipeline = \"default\";"
    },
    {
        "regex": "^rc$",
        "script": "let render_pipeline = \"custom\";"
    }
]
